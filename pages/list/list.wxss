@import "/common/list/list.wxss";

.panel {
  padding: 0;
  margin: 20rpx 0;
  background-color: #fff;
}

.panel > .h4 {
  height: 60rpx;
  padding: 0 30rpx;
  line-height: 60rpx;
  text-align: center;
  color: #333;
  font-size: 32rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}

.panel > .h4 {
  border-bottom: 1rpx solid #eee;
}

.panel .content {
  display: flex;
  flex-direction: colum;
  flex-wrap: wrap;
  width: 100%;
}


